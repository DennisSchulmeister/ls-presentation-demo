{
    "name": "ls-presentation-template",
    "version": "1.0.3",
    "description": "lecture-slides.js presentation demo",
    "author": "Dennis Schulmeister-Zimolong",
    "license": "BSD-3-Clause",
    "homepage": "https://github.com/DennisSchulmeister/ls-presentation-demo#readme",
    "bugs": {
        "url": "https://github.com/DennisSchulmeister/ls-presentation-demo/issues"
    },
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git@github.com:DennisSchulmeister/ls-presentation-demo.git"
    },
    "keywords": [
        "slideshow",
        "presentation",
        "lecture-slide.js",
        "template",
        "demonstration"
    ],
    "scripts": {
        "clean": "node scripts/clean.js",
        "build:static": "npm run clean; node scripts/build-static.js",
        "build:webpack": "webpack",
        "build": "npm run build:static; npm run build:webpack",
        "watch": "npm run build:static; watch-run -p 'static/**' npm run build:static",
        "devserver": "npm run watch & webpack-dev-server --content-base build/",
        "staticserver": "npm run build; http-server ./build"
    },
    "config": {
        "src_dir": "src",
        "static_dir": "static",
        "build_dir": "build",
        "public_url": "",
        "replace_variables_extensions": ".htm; .html"
    },
    "devDependencies": {
        "css-loader": "^0.28.11",
        "extract-text-webpack-plugin": "^3.0.2",
        "file-loader": "^0.11.2",
        "html-loader": "^0.5.5",
        "http-server": "^0.10.0",
        "lecture-slides.js": "^1.0.5",
        "less": "^2.7.3",
        "less-loader": "^4.1.0",
        "ls-plugin-highlight.js": "^1.0.2",
        "shelljs": "^0.7.8",
        "style-loader": "^0.18.2",
        "uglify-es": "git://github.com/mishoo/UglifyJS2#harmony",
        "uglifyjs-webpack-plugin": "^0.4.6",
        "url-loader": "^0.5.9",
        "watch-run": "^1.2.5",
        "webpack": "^3.12.0",
        "webpack-dev-server": "^2.11.3"
    }
}
